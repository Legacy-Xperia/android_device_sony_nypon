+int get_battery_level(void) {
+    char buf[4];
+    char *str;
+    int fd;
+    int level;
+    ssize_t nbytes;
+
+    fd = open(BATTERY_LEVEL_FILE, O_RDONLY);
+    if (fd < 0)
+        return 0;
+
+    nbytes = read(fd, buf, sizeof(buf) - 1);
+    close(fd);
+    if (nbytes < 0)
+        return 0;
+    buf[nbytes] = '\0';
+
+    str = strndup(buf, nbytes);
+    level = atoi(str);
+
+    return level;
+}
